{
    "name": "malikad778/notification-center",
    "description": "Enterprise-grade multi-channel notification engine for Laravel 12 with parallel dispatching, smart routing, and real-time monitoring.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notifications",
        "multichannel",
        "twilio",
        "fcm",
        "slack",
        "whatsapp",
        "parallel",
        "concurrency",
        "pennant",
        "pulse"
    ],
    "autoload": {
        "psr-4": {
            "malikad778\\NotificationCenter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "malikad778",
            "email": "malikad778@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/notifications": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/events": "^12.0",
        "illuminate/queue": "^12.0",
        "laravel/pennant": "^1.11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "malikad778\\NotificationCenter\\NotificationCenterServiceProvider"
            ],
            "aliases": {
                "NotificationCenter": "malikad778\\NotificationCenter\\Facades\\NotificationCenter"
            }
        }
    }
}